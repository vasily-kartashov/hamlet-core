{
  "name": "hamlet/hamlet-core",
  "type": "library",
  "description": "hamlet framework",
  "keywords": ["framework"],
  "version": "2.1.1",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Vasily Kartashov",
      "email": "vasily@kartashov.com"
    },
    {
      "name": "Danny Marsland",
      "email": "danny.marsland@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "psr/cache": "1.*",
    "psr/http-message": "1.*",
    "psr/log": "1.*",
    "guzzlehttp/psr7": "1.*",
    "symfony/console": "3.*",
    "twig/twig": "1.*"
  },
  "require-dev": {
    "cache/void-adapter": "^1",
    "cache/array-adapter": "^1",
    "phpunit/phpunit": "^6",
    "phpstan/phpstan": "*",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Hamlet\\": "src/Hamlet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hamlet\\": "tests/Hamlet"
    }
  }
}
